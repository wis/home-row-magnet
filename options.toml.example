font_family = "Ubuntu Mono"
[permutation]
keys = "asdfghjklnvmcu"
length = 4
# these are good settings for a 185 pixels per inch (PPI) 4K display/monitor
[shortcuts]
move = "meta+alt+m"
left_click = "meta+ctrl+c"
middle_click = "meta+ctrl+m"
right_click = "meta+alt+c"
drag_left = "meta+ctrl+d"
drag_right = "meta+alt+d"
cancel = ";"
